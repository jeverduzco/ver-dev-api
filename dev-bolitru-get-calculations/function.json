{
  "bindings": [
    {
      "authLevel": "function",
      "type": "httpTrigger",
      "direction": "in",
      "name": "req",
      "methods": [
        "get"
      ]
    },
    {
      "type": "http",
      "direction": "out",
      "name": "res"
    },
    {
      "type": "cosmosDB",
      "name": "calculations",
      "databaseName": "verduzco-dev-one-dev-dev",
      "collectionName": "bolitru-client-data",
      "connectionStringSetting": "DB",
      "direction": "in",
      "partitionKey": "{key}",
      "sqlQuery": "SELECT c.id, c.name, c.description FROM c WHERE c.project = {project} AND c.type = 'calculation' ORDER BY c.created DESC"
    }
  ]
}