{
  "bindings": [
    {
      "authLevel": "function",
      "type": "httpTrigger",
      "direction": "in",
      "name": "req",
      "methods": [
        "get"
      ]
    },
    {
      "type": "http",
      "direction": "out",
      "name": "res"
    },
    {
      "type": "cosmosDB",
      "name": "calculations",
      "databaseName": "verduzco-dev-one",
      "collectionName": "bolitru-client-data",
      "connectionStringSetting": "DB",
      "direction": "in",
      "sqlQuery": "SELECT c.id, c.name FROM Calculations c WHERE c.project = {project} AND c.owner = {owner} AND c.type = 'calculation' ORDER BY c.created DESC"
    }
  ]
}